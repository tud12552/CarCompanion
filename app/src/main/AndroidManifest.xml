<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.psusweng.carcompanion">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CarMaintHighLevel" />
        <activity android:name=".AddCarActivity" />
        <activity
            android:name=".MaintenanceActivity"
            android:label="@string/CarMaintHighTitleStr" />
        <activity android:name=".CarSelectActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StatsActivity"
            android:label="@string/StatsHeading" />
        <activity android:name=".ListViewCars" />
        <activity android:name=".SignIn"></activity>
        <activity android:name=".WebViewActivity"></activity>
    </application>

</manifest>